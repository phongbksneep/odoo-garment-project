<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_garment_packing_list" model="ir.sequence">
            <field name="name">Garment Packing List</field>
            <field name="code">garment.packing.list</field>
            <field name="prefix">PKL/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_garment_shipping_instruction" model="ir.sequence">
            <field name="name">Garment Shipping Instruction</field>
            <field name="code">garment.shipping.instruction</field>
            <field name="prefix">SI/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_garment_certificate_origin" model="ir.sequence">
            <field name="name">Garment Certificate of Origin</field>
            <field name="code">garment.certificate.origin</field>
            <field name="prefix">CO/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
