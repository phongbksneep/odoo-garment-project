<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_invoice_sale" model="ir.sequence">
            <field name="name">Garment Sale Invoice</field>
            <field name="code">garment.invoice.sale</field>
            <field name="prefix">INV-S-%(year)s-</field>
            <field name="padding">5</field>
        </record>
        <record id="seq_invoice_purchase" model="ir.sequence">
            <field name="name">Garment Purchase Invoice</field>
            <field name="code">garment.invoice.purchase</field>
            <field name="prefix">INV-P-%(year)s-</field>
            <field name="padding">5</field>
        </record>
        <record id="seq_payment" model="ir.sequence">
            <field name="name">Garment Payment</field>
            <field name="code">garment.payment</field>
            <field name="prefix">PAY-%(year)s-</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
