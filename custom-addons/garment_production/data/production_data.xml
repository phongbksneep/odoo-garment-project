<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_production_order" model="ir.sequence">
        <field name="name">Garment Production Order</field>
        <field name="code">garment.production.order</field>
        <field name="prefix">PO-%(year)s-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_cutting_order" model="ir.sequence">
        <field name="name">Garment Cutting Order</field>
        <field name="code">garment.cutting.order</field>
        <field name="prefix">CO-%(year)s-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
