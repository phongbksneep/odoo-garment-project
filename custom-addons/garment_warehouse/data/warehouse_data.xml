<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_stock_in" model="ir.sequence">
            <field name="name">Garment Stock In</field>
            <field name="code">garment.stock.move.in</field>
            <field name="prefix">NK-%(year)s-</field>
            <field name="padding">5</field>
        </record>
        <record id="seq_stock_out" model="ir.sequence">
            <field name="name">Garment Stock Out</field>
            <field name="code">garment.stock.move.out</field>
            <field name="prefix">XK-%(year)s-</field>
            <field name="padding">5</field>
        </record>
        <record id="seq_stock_transfer" model="ir.sequence">
            <field name="name">Garment Stock Transfer</field>
            <field name="code">garment.stock.move.transfer</field>
            <field name="prefix">CK-%(year)s-</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
